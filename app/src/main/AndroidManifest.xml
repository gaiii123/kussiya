        <activity android:name="com.example.kussiya.login" />
        <activity android:name="com.example.kussiya.signup" />
        <activity android:name="com.example.kussiya.account" />
        <activity android:name="com.example.kussiya.notification" />
        <activity android:name="com.example.kussiya.breakfast_screen" />
        <activity android:name="com.example.kussiya.DetailsActivity" />
        <activity
            android:name="com.example.kussiya.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
