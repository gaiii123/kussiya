<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.Kussiya" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorOnSecondary">@color/black</item>

    </style>
    <string-array name="category_array">
        <item>Breakfast</item>
        <item>Lunch</item>
        <item>Dinner</item>
        <item>Dessert</item>
    </string-array>
    <style name="Theme.Kussiya" parent="Base.Theme.Kussiya" />

    <style name="MyRatingBar" parent="Widget.AppCompat.RatingBar">
        <!--non selected star color-->
        <item name="colorControlNormal">#4D000000</item>
        <!-- selected star color-->
        <item name="colorControlActivated">@color/Screen</item>
    </style>

</resources>